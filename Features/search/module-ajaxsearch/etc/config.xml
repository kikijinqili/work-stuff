<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <ajaxsearch>
            <main>
                <enable>1</enable>
                <!-- <placeholder>Search entire store here...</placeholder> -->
                <limit>10</limit>
                <highlight>1</highlight>
                <hint>1</hint>
                <minLength>0</minLength>
                <classNames>
                    <input>tt-input</input>
                    <hint>tt-hint</hint>
                    <menu>tt-menu block-swissup-ajaxsearch-results</menu>
                    <dataset>tt-dataset wrapper</dataset>
                    <!-- <dataset>tt-dataset wrapper grid</dataset> -->
                    <!-- <dataset>tt-dataset wrapper list</dataset> -->
                    <suggestion>tt-suggestion</suggestion>
                    <empty>tt-empty</empty>
                    <open>tt-open</open>
                    <cursor>tt-cursor</cursor>
                    <highlight>tt-highlight</highlight>
                </classNames>
            </main>
            <design>
                <layout>grid</layout>
            </design>
            <folded>
                <enable>0</enable>
            </folded>
            <product>
                <enable>1</enable>
                <limit>12</limit>
            </product>
            <category>
                <filter>0</filter>
                <enable>1</enable>
                <limit>10</limit>
            </category>
            <page>
                <enable>1</enable>
                <limit>10</limit>
            </page>
            <autocomplete>
                <enable>1</enable>
                <show_on_focus>0</show_on_focus>
                <limit>10</limit>
            </autocomplete>
            <mysql>
                <search_method>fulltext</search_method>
                <like_words_limit>5</like_words_limit>
            </mysql>
        </ajaxsearch>
    </default>
</config>