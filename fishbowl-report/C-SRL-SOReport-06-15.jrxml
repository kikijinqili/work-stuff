	<subDataset name="LineData" uuid="439f672f-bec6-4dd9-a1ee-732c27b05fa9">
		<parameter name="soid" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[0]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select
	soitem.solineitem as "Item No.",
	soitem.productnum as "Catalog No.",
	soitem.description as "Description",
	soitem.note,
	product.details,
          soitem.showitemflag,

	soitem.qtytofulfill as "Qty",
	soitem.unitprice as "Unit Price",
	(soitem.qtytofulfill * soitem.unitprice) as "Total"
from soitem
	left join product on product.id = soitem.productid

where soitem.soid = $P{soid}
and not ( soitem.typeid in (40, 60) or soitem.productnum in ('0000','0001') )
and soitem.showitemflag = 1
and lower(soitem.note) <> 'cancelled'
order by soitem.solineitem]]>
		</queryString>
		<field name="Item No." class="java.lang.Integer"/>
		<field name="Catalog No." class="java.lang.String"/>
		<field name="Description" class="java.lang.String"/>
		<field name="NOTE" class="java.lang.String"/>
		<field name="DETAILS" class="java.lang.String"/>
		<field name="SHOWITEMFLAG" class="java.lang.Integer"/>
		<field name="Qty" class="java.lang.Double"/>
		<field name="Unit Price" class="java.lang.Double"/>
		<field name="Total" class="java.lang.Double"/>
	</subDataset>
	<subDataset name="PaymentData" uuid="888a65cb-1fbd-416c-a229-ab9cbb0ad16f">
		<parameter name="soNum" class="java.lang.String"/>
		<queryString>
			<![CDATA[select
    postransaction.datetime,
    paymentmethod.name,
    postransaction.amount
from postransaction
    join paymentmethod on paymentmethod.id = postransaction.paymentmethodid

where postransaction.soid = (select id from so where num = '22085' )]]>
		</queryString>
		<field name="DATETIME" class="java.sql.Timestamp"/>
		<field name="NAME" class="java.lang.String"/>
		<field name="AMOUNT" class="java.lang.Double"/>
	</subDataset>
	<parameter name="soNum" class="java.lang.String">
		<defaultValueExpression><![CDATA["0"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
    so.id as soid,
    so.statusid sostatus,

    so.shiptoname as soldtoname,
    addressmultilinesoview.shiptoaddress1 as soldtoadr1,
    addressmultilinesoview.shiptoaddress2 as soldtoadr2,
    addressmultilinesoview.shiptoaddress3 as soldtoadr3,
    so.shiptocity as soldtocity,
    soldstate.code as soldtostate,
    so.shiptozip as soldtozip,
    countryconst.name as soldtoCountry,
    soldtophone.datus as soldtophone,
    soldtofax.datus as soldtofax,
    soldtoemail.datus as soldtoemail,

    so.billtoname as billtoname,
    (case
     when locate(char(10), so.billtoaddress) > 0 then substring(so.billtoaddress from 1 for (locate(char(10), so.billtoaddress) - 1))
     else so.billtoaddress
    end) as billtoadr1,
    (case
     when locate(char(10), so.billtoaddress) > 0 and locate(char(10), so.billtoaddress, locate(char(10), so.billtoaddress) + 1) = 0 then substring(so.billtoaddress from (locate(char(10), so.billtoaddress) + 1))
     when locate(char(10), so.billtoaddress) > 0 and locate(char(10), so.billtoaddress, locate(char(10), so.billtoaddress) + 1) > 0 then substring(so.billtoaddress from (locate(char(10), so.billtoaddress) + 1) for (locate(char(10), so.billtoaddress, locate(char(10), so.billtoaddress) + 1) - (locate(char(10), so.billtoaddress) + 1)))
     else ''
    end) as billtoadr2,
    (case
     when locate(char(10), so.billtoaddress, locate(char(10), so.billtoaddress) + 1) > 0 then substring(so.billtoaddress from locate(char(10), so.billtoaddress, locate(char(10), so.billtoaddress) + 1) + 1)
     else ''
    end) as billtoadr3,

    so.billtocity as billtocity,
    billstate.code as billtostate,
    so.billtozip as billtozip,
    billcountry.name as billcountry,
    so.customercontact as billtoattn,
    billtoemail.datus as billtoemail,
    billphone.datus as billphone,

    so.num as invoiceno,
(CASE WHEN so.num=69043 THEN CAST('2021-06-04' AS datetime)
WHEN so.num=69170 THEN CAST('2021-06-14' AS datetime)
WHEN so.num=68238 THEN CAST('2021-06-14' AS datetime)
ELSE so.datecompleted
END) AS invoicedate,
    so.customerpo as orderno,
    DATE_ADD(so.datecompleted, INTERVAL paymentterms.netdays DAY) as duedate,
    customer.number as customerno,

    lt.allitems-lt.shipping-lt.dryice as amtsubtotal,
    lt.shipping as amtshipping,
    lt.dryice as amtdryice,
    so.totaltax as amttax,
    lt.allitems+so.totaltax as amttotal,
    COALESCE(pt.amt,0) as amtpayments,
    (lt.allitems+so.totaltax)-COALESCE(pt.amt,0) as amtamountdue,
    pt.ct as paymentcount,
    billfax.datus as billtofax


from so
    left join addressmultilinesoview on addressmultilinesoview.soid = so.id
    join customer on customer.id = so.customerid
    left join paymentterms on paymentterms.id = so.paymenttermsid
    left join address on address.address = so.shiptoaddress and address.accountid = customer.accountid
    LEFT JOIN address billaddress on so.billtoaddress = billaddress.address and billaddress.accountid = customer.accountid
    left join address shipaddress on so.shiptoaddress = shipaddress.address and shipaddress.accountid = customer.accountid
    left join addressmultilineview on addressmultilineview.id = address.id
    left join contact as billfax on billfax.typeid = 40 and billfax.addressid = billaddress.id and billfax.defaultflag =1
    LEFT JOIN contact as billtoEmail on billtoemail.typeid = 60 and billtoemail.addressid = billaddress.id and billtoemail.defaultflag = 1
    left join contact as billphone on billphone.addressid = billaddress.id and billphone.defaultflag = 1 and billphone.typeid = 50
    left join contact as soldtophone on soldtophone.addressid = shipaddress.id and soldtophone.defaultflag = 1 and soldtophone.typeid = 50
    LEFT JOIN contact as soldtoemail on soldtoemail.typeid = 60 and soldtoemail.addressid = shipaddress.id and soldtoemail.defaultflag = 1
    left join contact as soldtofax on soldtofax.typeid = 40 and soldtofax.addressid = shipaddress.id and soldtofax.defaultflag =1
    LEFT JOIN stateConst billState ON so.billToStateId = billState.id
    LEFT JOIN stateConst soldState ON so.shiptostateid = soldstate.id
    LEFT JOIN countryconst on so.shiptocountryid = countryconst.id
    LEFT JOIN countryconst billcountry on so.billtocountryid = billcountry.id
    left join (select soid, count(id) as ct, sum(amount) amt from postransaction group by soid ) as pt on pt.soid = so.id
    left join (select
                        soitem.soid,
                        sum( IF( soitem.typeid <> 40 /* Subtotal lines */, soitem.qtytofulfill * soitem.unitprice, 0 ) ) as allitems,
                        SUM( IF( soitem.typeid = 60 /* Shipping Line Item TYpes */ , soitem.qtytofulfill * soitem.unitprice, 0 ) ) as shipping,
                        SUM( IF( soitem.productnum in ('0000','0001') , soitem.qtytofulfill * soitem.unitprice, 0 ) ) as dryice
                from soitem
                where lower(soitem.note) <> 'cancelled'
                group by soitem.soid) as lt on lt.soid = so.id

where so.num = $P{soNum}]]>
	</queryString>
